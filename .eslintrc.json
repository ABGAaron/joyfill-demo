{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "object-shorthand": "off",
    "multiline-ternary": "off"
  }
}
